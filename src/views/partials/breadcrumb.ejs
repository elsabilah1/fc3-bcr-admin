<div class="mb-6">
  <ol class="list-reset flex items-center">
    <li class="font-bold text-xs text-neutral05 ">
      <%= url=='/dashboard' ? 'Dashboard' : 'Cars' %>
    </li>
    <% url=url.split('/') %>
    <% let x='' %>
    <%url.forEach((u,i)=> {%>
    <% if(i!=0) {%>
    <li><i class="w-3" data-feather="chevron-right"></i></li>
    <li class="flex items-center">
      <% x=x.concat(`/${u}`) %>
      <a href='<%= x %> ' class="capitalize font-light text-xs text-neutral05 hover:text-darkblue04">
        <%= u.split('?')[0].replace(/-/g,' ') %>
      </a>
    </li>
    <% }}) %>
  </ol>
</div>
<script>
  feather.replace()
</script>